{
	"name": "vscode-openvadl",
	"displayName": "OpenVADL",
	"description": "Syntax highlighting for the VADL language",
	"version": "0.0.4",
	"publisher": "OpenVADL",
	"license": "SEE LICENSE IN LICENSE.txt",
	"repository": {
		"type": "git",
		"url": "https://github.com/OpenVADL/vscode-openvadl"
	},
	"homepage": "https://openvadl.org",
	"categories": [
		"Programming Languages"
	],
	"keywords": [
		"vadl",
		"processor"
	],
	"icon": "openvadl.png",
	"preview": true,
	"engines": {
		"vscode": "^1.76.0"
	},
	"contributes": {
		"languages": [
			{
				"id": "vadl",
				"aliases": [
					"VADL",
					"vadl"
				],
				"extensions": [
					".vadl"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "vadl",
				"scopeName": "source.vadl",
				"path": "./syntaxes/vadl.tmLanguage.json"
			}
		]
	}
}